neighbor 2.0 bin #default bin value for unit style (ie. metal)
neigh_modify every 1 delay 0 check yes #rebuild the neighbor list at every timestep

variable Nfreq equal 1000
variable Nprint equal 1000
variable Nevery equal 1000
variable Nrepeat equal ${Nfreq}/${Nevery}
variable Nrepeat equal 1000/${Nevery}
variable Nrepeat equal 1000/1000
variable Ndump equal ${ndump}
variable Ndump equal 10000
#variable nAtoms equal atoms
#variable Nrestart equal ${quench_timesteps}/15

timestep 0.0001

fix removeMomentum all momentum 1 linear 1 1 1

compute T all temp
fix TempAve all ave/time ${Nevery} ${Nrepeat} ${Nfreq} c_T
fix TempAve all ave/time 1000 ${Nrepeat} ${Nfreq} c_T
fix TempAve all ave/time 1000 1 ${Nfreq} c_T
fix TempAve all ave/time 1000 1 1000 c_T

variable P equal press
fix PressAve all ave/time ${Nevery} ${Nrepeat} ${Nfreq} v_P
fix PressAve all ave/time 1000 ${Nrepeat} ${Nfreq} v_P
fix PressAve all ave/time 1000 1 ${Nfreq} v_P
fix PressAve all ave/time 1000 1 1000 v_P

variable v equal vol
fix vAve all ave/time ${Nevery} ${Nrepeat} ${Nfreq} v_v
fix vAve all ave/time 1000 ${Nrepeat} ${Nfreq} v_v
fix vAve all ave/time 1000 1 ${Nfreq} v_v
fix vAve all ave/time 1000 1 1000 v_v

compute PE all pe pair
fix PEAve all ave/time ${Nevery} ${Nrepeat} ${Nfreq} c_PE
fix PEAve all ave/time 1000 ${Nrepeat} ${Nfreq} c_PE
fix PEAve all ave/time 1000 1 ${Nfreq} c_PE
fix PEAve all ave/time 1000 1 1000 c_PE
compute PEa all pe/atom pair

compute MSD all msd

thermo_style custom step cpu temp f_TempAve press f_PressAve f_PEAve vol f_vAve c_MSD[4]
thermo_modify flush yes
thermo ${Nfreq}
thermo 1000


