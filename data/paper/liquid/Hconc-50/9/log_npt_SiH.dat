dump ens all custom ${Ndump} ${rundir}/NPT/dump_custom_pace id element x y z fx fy fz c_PEa #f_pace_gamma_reg
dump ens all custom 10000 ${rundir}/NPT/dump_custom_pace id element x y z fx fy fz c_PEa 
dump ens all custom 10000 /u/vld/spet5633/Silicon/a-Si_potential/DFT_1200eV/Testing/Paper/l-SiH/1,000_structures/NVT_2.58/Hconc-50/9/NPT/dump_custom_pace id element x y z fx fy fz c_PEa 
#difference between dump ens and dump sep is that dump sep has a .dat file for each timestep vs ens is 1 big file
dump_modify ens sort 1 #sorting the dump by the 1st column of data ie the id
dump_modify ens element H Si

